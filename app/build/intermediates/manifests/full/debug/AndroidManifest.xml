            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyAc9BAAvdRlLd6K1YP57MOUBaxOu4gvm0Y" />

        <activity
            android:name="com.google.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" >
        </activity>
        <activity
            android:name="com.YourPackage.EventApp.EventApp"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.YourPackage.EventApp.ActivityDetails"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.YourPackage.EventApp.Welcome" />
        <activity android:name="com.YourPackage.EventApp.Booking" />
        <activity android:name="com.YourPackage.EventApp.Shedule" />
        <activity android:name="com.YourPackage.EventApp.Map" />
        <activity android:name="com.YourPackage.EventApp.Social" />

        <receiver
            android:name="com.YourPackage.EventApp.Widget"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE" />
            </intent-filter>

            <meta-data
                android:name="android.appwidget.provider"
                android:resource="@xml/widget_provider" />
        </receiver>
        <!-- Include the AdActivity and InAppPurchaseActivity configChanges and themes. -->
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
